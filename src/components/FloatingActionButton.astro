<button
  floating-action-button
  type="button"
  commandfor="movements-modal"
  command="show-modal"
  aria-label="Add new movement"
  class="absolute bottom-14 right-0 z-50 hover:cursor-pointer rounded-full bg-sky-600"
>
  <svg
    viewBox="0 0 24 24"
    width="48"
    height="48"
    stroke="currentColor"
    xmlns="http://www.w3.org/2000/svg"
    ><g
      id="SVGRepo_bgCarrier"
      stroke-width="0"
    ></g><g
      id="SVGRepo_tracerCarrier"
      stroke-linecap="round"
      stroke-linejoin="round"
    ></g><g id="SVGRepo_iconCarrier">
      <g id="Edit / Add_Plus">
        <path
          id="Vector"
          d="M6 12H12M12 12H18M12 12V18M12 12V6"
          stroke="currentColor"
          stroke-width="2.4"
          stroke-linecap="round"
          stroke-linejoin="round"
        ></path>
      </g>
    </g></svg
  >
</button>
